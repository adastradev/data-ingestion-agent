{
  "name": "@adastradev/data-ingestion-agent",
  "version": "1.0.0",
  "description": "Ad Astra Docker agent code base for cloud integration without VPN tunnels",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc --require ts-node/register mocha --timeout 999999 test/unit/**/*.spec.ts",
    "integration-test": "nyc --require ts-node/register mocha --timeout 999999 test/integration/**/*.spec.ts",
    "system-test": "mocha -r ts-node/register --timeout 999999 test/system/**/*.spec.ts",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "node dist/start.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "pipeline/**",
      "coverage/**",
      "dist/**",
      "test/**",
      "webpack.config.js",
      "source-map-install.js"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ],
    "sourceMap": true,
    "all": true
  },
  "author": "Ad Astra",
  "contributors": [
    "Cory Routledge <croutledge@aais.com>",
    "Andrew Regier <aregier@regiernet.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@adastradev/serverless-discovery-sdk": "0.0.7",
    "@types/chai": "^4.1.6",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/shelljs": "^0.8.0",
    "@types/sinon": "^5.0.5",
    "@types/ssh2": "^0.5.36",
    "amazon-cognito-identity-js": "^3.0.2",
    "aws-api-gateway-client": "^0.2.16",
    "aws-sdk": "^2.329.0",
    "aws-sdk-mock": "^4.2.0",
    "crypto": "^1.0.1",
    "csv-stringify": "^4.3.1",
    "express": "^4.16.3",
    "inversify": "^4.13.0",
    "node-fetch": "^2.2.0",
    "node-ssh": "^5.1.2",
    "oracledb": "^3.0.0",
    "shelljs": "^0.8.2",
    "stream-transform": "^1.0.7",
    "through2": "^2.0.3",
    "ts-loader": "^5.2.1",
    "typescript": "^3.0.3",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/oracledb": "^1.11.34",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "install": "^0.12.1",
    "mocha": "^5.2.0",
    "npm": "^6.4.1",
    "nyc": "^13.0.1",
    "sinon": "^7.0.0",
    "ssh2": "^0.6.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-no-unused-expression-chai": "^0.1.4"
  }
}
