{
  "name": "@adastradev/data-ingestion-agent",
  "version": "1.6.0",
  "description": "Ad Astra Docker agent code base for cloud integration without VPN tunnels",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --sourceMap true",
    "lint": "tslint {source,test}/**/*.ts",
    "test:clean": "npm run test:clean-coverage && npm run test:clean-nyc",
    "test:clean-coverage": "rimraf ./coverage",
    "test:clean-nyc": "rimraf ./.nyc_output",
    "test:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test:coverage-check": "nyc check-coverage",
    "test:coverage-report": "nyc report --reporter=\"text-summary\"",
    "test": "npm run test:clean && nyc --require ts-node/register mocha --timeout 300000 \"test/{unit,integration}/**/*.spec.ts\"",
    "snyk:protect": "snyk protect",
    "unit-test": "npm run test:clean && nyc --require ts-node/register mocha --timeout 999999 test/unit/**/*.spec.ts",
    "integration-test": "nyc --require ts-node/register mocha --timeout 120000 test/integration/**/*.spec.ts",
    "system-test": "mocha -r ts-node/register --timeout 999999 test/system/**/*.spec.ts",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "node dist/start.js"
  },
  "pre-commit": [
    "unit-test"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "pipeline/**",
      "coverage/**",
      "dist/**",
      "test/**",
      "webpack.config.js",
      "source-map-install.js",
      "inversify.config.ts",
      "**/I[A-Z]*.ts",
      "source/Util/pre-commit.ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ],
    "sourceMap": true,
    "all": true,
    "clean": false,
    "cache": false
  },
  "author": "Ad Astra",
  "contributors": [
    "Cory Routledge <croutledge@aais.com>",
    "Andrew Regier <aregier@regiernet.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@adastradev/aws-api-gateway-client": "0.2.18-beta.3",
    "@adastradev/data-ingestion-sdk": "^2.0.0",
    "@adastradev/serverless-discovery-sdk": "^2.0.2",
    "@adastradev/user-management-sdk": "^2.0.1",
    "@types/bluebird-global": "^3.5.9",
    "@types/chai": "^4.1.6",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/shelljs": "^0.8.0",
    "@types/sinon": "^5.0.5",
    "@types/ssh2": "^0.5.36",
    "amazon-cognito-identity-js": "^3.0.2",
    "async": "^2.6.1",
    "aws-sdk": "^2.329.0",
    "aws-sdk-mock": "^4.2.0",
    "axios": "^0.19.0",
    "crypto": "^1.0.1",
    "dependency-graph": "^0.7.2",
    "express": "^4.16.3",
    "fetch-with-proxy": "^1.1.0",
    "inquirer": "^7.0.0",
    "inversify": "^4.13.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "node-fetch": "^2.2.0",
    "node-ssh": "^5.1.2",
    "oracledb": "^3.0.0",
    "proxy-agent": "^3.0.3",
    "shelljs": "^0.8.2",
    "snyk": "^1.228.5",
    "through2": "^2.0.3",
    "ts-loader": "^5.2.1",
    "typescript": "^3.0.3",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/lodash": "^4.14.138",
    "@types/oracledb": "^1.11.34",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "install": "^0.12.1",
    "mocha": "^5.2.0",
    "npm": "^6.13.4",
    "nyc": "^14.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "sinon": "^7.0.0",
    "ssh2": "^0.6.1",
    "ts-node": "^7.0.1",
    "ts-sinon": "^1.0.24",
    "tslint": "^5.11.0",
    "tslint-config-security": "^1.16.0",
    "tslint-no-unused-expression-chai": "^0.1.4"
  },
  "cloudDependencies": {
    "elt-queries": "1.x"
  }
}
