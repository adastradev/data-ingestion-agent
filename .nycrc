{
  "extends": "@istanbuljs/nyc-config-typescript",
  "require": [
    "ts-node/register"
  ],
  "exclude": [
    "**/*.d.ts",
    "pipeline/**",
    "coverage/**",
    "dist/**",
    "test/**",
    "webpack.config.js",
